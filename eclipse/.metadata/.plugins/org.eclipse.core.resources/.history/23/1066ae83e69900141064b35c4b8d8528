package com.miner3389.nerdspeaq.client.settings;

import net.minecraft.client.settings.KeyBinding;

import org.lwjgl.input.Keyboard;

import com.miner3389.nerdspeaq.reference.NSNamesReference;
import com.miner3389.nerdspeaq.reference.NSNamesReference.Keys;

public class NSKeyBindings {
	public static KeyBinding charge = new KeyBinding(NSNamesReference.Keys.CHARGE, Keyboard.KEY_C, NSNamesReference.Keys.CATAGORY);
	public static KeyBinding release = new KeyBinding(NSNamesReference.Keys.RELEASE, Keyboard.KEY_V, NSNamesReference.Keys.CATAGORY);
}
